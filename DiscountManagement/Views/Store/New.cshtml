
@{
    ViewBag.Title = "New";
}

<h2>Create Store</h2>
<form action="/Store/Create" method="POST">
    <div>
        <label>Name:</label> <input type="text" name="StoreName" />
    </div>
    <div>
        <label>Discount code:</label> <input type="text" name="DiscountCode" />
    </div>
    <div>
        <label>Discount rate:</label> <input type="text" name="DiscountPercentage" />
    </div>
    <div>
        <input type="submit" value="Add Store"/>
    </div>

</form>

